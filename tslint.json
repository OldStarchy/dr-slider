{
	"defaultSeverity": "error",
	"extends": ["tslint:recommended", "tslint-config-prettier"],
	"jsRules": {},
	"rules": {
		"indent": {
			"options": "tabs",
			"severity": "error"
		},
		"quotemark": {
			"options": "single",
			"severity": "warning"
		},
		"interface-name": false,
		"no-var-keyword": {
			"severity": "error"
		},
		"no-console": false,
		"no-namespace": false
	},
	"rulesDirectory": []
}
